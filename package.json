{
  "name": "video-extract",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/kurz-net/video-extract.git",
  "author": "Daniel Kurz <danielkurz03@protonmail.com>",
  "private": true,
  "license": "MIT",
  "workspaces": [
    "apps/*",
    "packages/*",
    "prisma"
  ],
  "scripts": {
    "dev:web": "dotenv -e .env -- yarn --cwd apps/web run dev",
    "dev:down:api": "dotenv -e .env -- yarn --cwd apps/downloader run dev:api",
    "dev:down:down": "dotenv -e .env -- yarn --cwd apps/downloader run dev:down",
    "dev:all": "dotenv -e .env -- run-p dev:web dev:down:*",
    "lint": "eslint **/*.ts*",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "db:gen": "prisma generate",
    "db:seed": "prisma db seed",
    "db:clear": "ts-node prisma/clear.ts"
  },
  "devDependencies": {
    "@types/node": "^18.6.2",
    "prettier": "^2.7.1",
    "prisma": "^4.2.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@prisma/client": "^4.2.1",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.20.0",
    "npm-run-all": "^4.1.5"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
